{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": ["eslint:all", "plugin:react/recommended", "plugin:@typescript-eslint/recommended"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest/globals": true
    },
    "rules": {
        "@typescript-eslint/no-shadow": ["warn", { "allow": ["_"] }],
        "array-element-newline": 0,
        "arrow-parens": 0,
        "complexity": 0,
        "curly": 0,
        "dot-location": 0,
        "function-call-argument-newline": 0,
        "function-paren-newline": 0,
        "id-length": 0,
        "implicit-arrow-linebreak": 0,
        "lines-between-class-members": 0,
        "max-classes-per-file": 0,
        "max-depth": 0,
        "max-len": 0,
        "max-lines-per-function": 0,
        "max-lines": 0,
        "max-params": 0,
        "max-statements": 0,
        "multiline-ternary": 0,
        "newline-per-chained-call": 0,
        "no-console": 0,
        "no-continue": 0,
        "no-extra-parens": 0,
        "no-loop-func": 0,
        "no-magic-numbers": 0,
        "no-mixed-operators": 0,
        "no-plusplus": 0,
        "no-shadow": 0,
        "no-ternary": 0,
        "no-undefined": 0,
        "no-use-before-define": 0,
        "nonblock-statement-body-position": 0,
        "object-curly-spacing": 0,
        "object-property-newline": 0,
        "one-var": 0,
        "padded-blocks": 0,
        "prefer-destructuring": 0,
        "quote-props": 0,
        "react/display-name": 0,
        "react/no-unescaped-entities": 0,
        "react/prop-types": 0,
        "sort-keys": 0,
        "space-before-function-paren": 0,
        "spaced-comment": 0
    },
    "plugins": ["jest", "@typescript-eslint", "only-warn"]
}
